{
  "name": "ReactFLORA",
  "version": "0.1.1",
  "author": "Greg Gleason",
  "description": "FLORA is a Desktop app utilizing React Native and Google Firestore to manage PORs at every level. Special thanks to Tamjid Hossain for his extensive help when resolving issues and feature additions",
  "private": true,
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "concurrently": "^6.4.0",
    "firebase": "^9.5.0",
    "react": "^16.13.1",
    "react-bootstrap": "^2.0.2",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "wait-on": "^6.0.0"
  },
  "devDependency": {
    "electron": "^16.0.4",
    "electron-builder": "^22.14.5"
  },
  "main": "public/main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none npm run start\" \"npm run electron:start\"",
    "electron:build": "npm run build && electron-builder -c.extraMetadata.main=build/main.js",
    "electron:start": "wait-on tcp:3000 && electron ."
  },
  "build": {
    "extends": null,
    "appId": "com.reactflora.electron",
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "icon": "src/FLORA.ico"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
